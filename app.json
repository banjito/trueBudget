    "android": {
      "package": "com.cohn.truebudget"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "owner": "cohn",
    "extra": {
      "eas": {
        "projectId": "640b5e99-3cfe-4500-94a3-18f815d28b0f"
      }
    }
  }
